configuration {
	font: "Hack 16";
	terminal: "urxvt";
	cache-dir: "/home/tpalmerstudios/.cache";
	window-thumbnail: true;
	display-drun: "Apps";
	display-files: "Files";
	display-run: "ZSH";
	drun-match-fields: "name,generic,exec,categories,keywords";
	show-icons: true;
	icon-theme: "Papirus";
	drun-display-format: "{name}[<span weight='light'><i> - ({generic})</i></span>]";
	
}
filebrowser {
	directory: "/home/tpalmerstudios/";
	directories-first: true;
	sorting-method:    "name";
}
@theme "solarized"
